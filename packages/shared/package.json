{
  "name": "@econeura/shared",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types/index.js",
    "./schemas": "./dist/schemas/index.js",
    "./src/schemas": "./dist/schemas/index.js",
    "./clients": "./dist/clients/index.js",
    "./security": "./dist/security/index.js",
    "./logging": "./dist/logging/index.js",
    "./metrics": "./dist/metrics/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "lint": "eslint . --max-warnings 25",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:unit": "vitest --run --reporter verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "dup": "jscpd --format json --output reports/jscpd.json --min-lines 10 --min-tokens 50 --reporters json src/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.5.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.7.0",
    "@types/redis": "^4.0.11",
    "@types/winston": "^2.4.4",
    "applicationinsights": "^3.9.0",
    "crypto": "^1.0.1",
    "pino": "^10.0.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.10",
    "uuid": "^13.0.0",
    "winston": "^3.13.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "globals": "^16.4.0",
    "jscpd": "^4.0.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.4"
  }
}
